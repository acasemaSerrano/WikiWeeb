<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.acasema.wikiweeb">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_wiki_weeb"
        android:label="@string/app_name"
        android:launchMode="singleTask"
        android:roundIcon="@mipmap/ic_wiki_weeb_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".iu.BaseActivity"></activity>
        <activity android:name=".iu.review.ReviewActivity"></activity>
        <activity android:name=".iu.SignUpActivity" />
        <activity
            android:name=".iu.MainActivity"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".iu.LoginActivity"
            android:launchMode="singleTop" />
    </application>

</manifest>